<div class="col">
    <div class="col"
        style="margin-top: 20px; margin-bottom: 20px; font-family: 'Times New Roman', Times, serif">
        <div class="row">
            <!-- Các sản phẩm Quần của shop-->

            <!-- Card mỗi sản phẩm -->
            <% item.forEach(function(item){ %>
            <div class="col col-md-6 ">
                <div class="card" style="width: 18rem;">
                    <img src="<%= item.url%>" class="card-img-top" alt="<%= item.name%>" style="height: 300px">
                    <div class="card-body">
                        <a href="/chitietsanpham?id=<%= item.id%>">
                            <h5 class="card-title"><%= item.name%></h5>
                        </a>
                        <p class="card-text">Giá : <%- item.price%></p>
                        <a href="/chitietsanpham?id=<%= item.id%>" class="btn btn-primary">Chi Tiết </a>
                    </div>
                </div>
            </div>
            <% })%>

            <!-- Finish-->

        </div>
    </div>
    <!-- Các sản phẩm khác của shop-->
</div>
<!-- Dịch vụ và sản phẩm qua từng tháng-->
